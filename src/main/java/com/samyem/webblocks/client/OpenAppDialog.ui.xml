<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		
		<g:FlexTable ui:field="grid" styleName="selTable"></g:FlexTable>
		 
		 <div>
			<g:Button styleName="{style.important}" ui:field="btnOpen" text="Open"/>
			<g:Button styleName="{style.important}" ui:field="btnCancel" text="Cancel"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 